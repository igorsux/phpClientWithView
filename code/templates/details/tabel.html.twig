<table class="table table-striped table-bordered table-hover table-responsive">
    <thead>
    <tr>
        <th>способ</th>
        <th>Тип</th>
        <th>Статус</th>
        <th>Рассылка</th>
        <th>Имя</th>
        <th>Номер, оператор, страна, регион</th>
        <th>Текст сообщения</th>
        <th>Дата стутуса</th>
    </tr>
    </thead>
    <tbody>
    {% for item in items %}
        <tr>
            <td>{{ item.source }}</td>
            <td>{{ item.type }}</td>
            <td>{{ item.status }}</td>
            <td>{{ item.dispatchName }}</td>
            <td>{{ item.from }}</td>
            <td>{{ item.to }} {{ item.operator ?? "--" }} {{ item.country ?? "--" }} {{ item.regionId ?? "--" }}</td>
            <td>{{ item.text }}</td>
            <td>{{ item.time |date('Y-m-d H:s') }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
